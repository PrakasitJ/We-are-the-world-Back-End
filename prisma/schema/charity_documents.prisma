model Charity_documents {
    id                                  Int      @id @default(autoincrement())
    charity_id                          Int      @unique
    certificate_of_registration_url     String
    list_of_board_members_url           String
    tax_exemption_url                   String
    proof_of_the_foundation_address_url String
    createdAt                           DateTime @default(now())
    updatedAt                           DateTime @updatedAt

    charity Charity @relation(fields: [charity_id], references: [id])
}
