model Product_list {
    quantity  Int
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt

    order    Order @relation(fields: [order_id], references: [id])
    order_id Int
    product    Product @relation(fields: [product_id], references: [id])
    product_id Int

    @@unique([order_id, product_id])
}
